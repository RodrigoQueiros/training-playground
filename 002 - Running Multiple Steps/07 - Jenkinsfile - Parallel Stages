pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
    	stage('Parallel Stage') {
            parallel {
                stage('Node') {
                    steps {
                        sh 'npm --version'
                    }
                }
                stage('MVN') {
                    steps {
                        sh 'mvn --version'
                    }
                }
    	    }
    	}
    }
}
